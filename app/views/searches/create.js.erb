
$("<div id='results'> <%= if @search.results[0..9].empty?
 escape_javascript(render partial:  "results/no_result")
else
escape_javascript(render partial:  "results/result" , collection: @search.results[0..9])
end
%></div>").replaceAll("#results");

$('#search_icon').removeClass('glyphicon-refresh spinning');

$(function () {
    $("div").on("swipe", swipeHandler);

    function swipeHandler(event, id) {

        $(id).remove();

    }
});





